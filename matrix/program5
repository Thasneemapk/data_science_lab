import numpy as np
ar1=np.array([[2,4],[4,6]])
ar2=np.array([[3,5],[5,2]])
print(ar1)
print(ar2)
print("sum=",np.add(ar1,ar2))
print("substarction=",np.subtract(ar1,ar2))
print("multiplication=",np.multiply(ar1,ar2))


